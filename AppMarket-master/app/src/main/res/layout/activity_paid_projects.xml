<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:toolbar="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    android:id="@+id/paid_project_fragment_page"
    android:orientation="vertical"
    tools:context="com.example.admin.appmarket.activity.PaidProjects">

        <android.support.v7.widget.Toolbar
            android:id="@+id/paid_project_back_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            toolbar:title="已购项目"
            toolbar:navigationIcon="@mipmap/back"
            android:background="@color/colorMine">
        </android.support.v7.widget.Toolbar>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/bg">

                <android.support.v4.view.ViewPager
                    android:id="@+id/paid_project_viewpager"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true" />
        </LinearLayout>




</LinearLayout>